   ],
   "source": [
    "# get plinder index with selected annotation columns specified\n",
    "plindex = query_index(\n",
    "    columns=[\n",
    "        \"system_id\",\n",
    "        \"ligand_id\",\n",
    "        \"entry_pdb_id\",\n",
    "        \"ligand_rdkit_canonical_smiles\",\n",
    "        \"system_num_ligand_chains\",\n",
    "        \"ligand_is_proper\",\n",
    "        \"ligand_ccd_code\",\n",
    "    ],\n",
    "    splits=[\"*\"],\n",
    "    filters=[(\"entry_pdb_id\", \"in\", pb_pdb_list)]\n",
    ")\n",
    "plindex['PDB_CCD'] = [f\"{p.upper()}_{c.upper()}\" for p, c in plindex[['entry_pdb_id', \"ligand_ccd_code\"]].values]"
   ]
  },
