  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "308\n"
     ]
    }
   ],
   "source": [
    "if not AnyPath(\"posebusters_pdb_ccd_ids.txt\").exists():\n",
    "    os.system(\"wget https://github.com/maabuu/posebusters/files/14516485/posebusters_pdb_ccd_ids.txt\")\n",
    "posebusters = pd.read_csv(\"posebusters_pdb_ccd_ids.txt\", names=['PDB_CCD'])\n",
    "posebusters.loc[posebusters.PDB_CCD == \"7D6O_MTE\", \"PDB_CCD\"] = \"8J79_MTE\"\n",
    "pb_pdb_list = posebusters[\"PDB_CCD\"].apply(lambda x: x.split(\"_\")[0].lower()).to_list()\n",
    "print(len(pb_pdb_list))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
